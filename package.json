{
  "name": "ventilation",
  "version": "1.0.0",
  "description": "A landing page for a ventilation company",
  "scripts": {
    "lint:js": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "lint:css": "stylelint src/*.css",
    "test": "npm run lint:js && npm run lint:css",
    "build:css": "postcss src/**/*.css  --base src --dir build --verbose",
    "build:js": "babel src/**/*.js -o build/app.js",
    "build": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darco2018/ventilation.git"
  },
  "author": "Dariusz Ustrzycki <ustrzycki@javacoder.cloud>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darco2018/ventilation.git/issues"
  },
  "homepage": "http://www.javacoder.cloud",
  "dependencies": {
    "bootstrap": "4.3.1",
    "jquery": "^3.4.0",
    "popper.js": "1.14.7",
    "tether": "1.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "apache-server-configs": "^3.1.0",
    "archiver": "^3.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "del": "^4.1.0",
    "doiuse": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "glob": "^7.1.3",
    "htmlhint": "^0.11.0",
    "mocha": "^5.2.0",
    "modernizr": "3.7.1",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "ssri": "^6.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "h5bp-configs": {
    "directories": {
      "archive": "archive",
      "dist": "dist",
      "src": "src",
      "test": "test"
    }
  },
  "files": [
    "LICENSE.txt",
    "package.json",
    "dist/",
    "README.md"
  ],
  "babel": {
    "presets": ["@babel/preset-env"]
  },
  "browserslist": [
    "last 2 versions and > 1.0% in Pl",
    "Edge > 16",
    "Firefox ESR",
    "maintained node versions",
    "not dead"
  ]
